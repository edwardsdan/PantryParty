@using PantryParty.Models;
@{
    ViewBag.Title = "NearbyUsers";
}

<h2>List of nearby users with ingredients you're missing</h2>

@foreach (AspNetUser user in ViewBag.NearbyUsers)
{
    <h2>@user.FirstName @user.LastName</h2><br />
    <h3>@user.City</h3><br />
    <h3>@user.Email</h3><br />
    <h5>@user.PhoneNumber</h5><br />

}

