
@{
    ViewBag.Title = "ShowResults";
}

<!--example table-->

<table class="table">
    <tr>
        <th><h4></h4></th>
        <th><h4>Header1</h4></th>
        <th><h4>Header2</h4></th>
        <th><h4>Header3</h4></th>

    </tr>
    @for (int i = 0; i < 12; i++)
    {
        <tr>
            <td><img src="@ViewBag.Pic[i]" width="100" height="100" /></td>
            <td>@ViewBag.Time[i]</td>
            <td>@ViewBag.DataWeather[i]</td>
            <td>@ViewBag.Data[i]</td>
        </tr>
    }

</table>

